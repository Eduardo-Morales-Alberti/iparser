<?xml version='1.0' encoding='utf-8'?>
<widget id="org.edu.iparser" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>IParser</name>
    <icon src="res/icon/android/icon-48-mdpi.png" />
    <description>
        An app to parser vocals to i vocal.
    </description>
    <author email="eduardomoberti@hotmail.com" href="https://github.com/Eduardomoberti">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="res/icon/android/icon-48-mdpi.png" />
        <splash density="port-hdpi" src="res/screen/android/screen-hdpi-portrait.png" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="cordova-plugin-dialogs" spec="^2.0.1" />
    <engine name="android" spec="^7.1.0" />
</widget>
